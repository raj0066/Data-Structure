//find largest number and it's index 

#include<bits/stdc++.h>
 using namespace std;
 int main()
 {
    int n;
    cin>>n;
    int ar[n];
    for(int i=1;i<=n;i++){
        cin>>ar[i];
    }
 int max = ar[1];
    for(int i=2;i<=n;i++){
        if(max<ar[i]){
            max = ar[i];
        }
    }
  for(int i=1;i<=n;i++){
        if(ar[i]==max)
        {
        }
    }
            cout<<max<<endl;
            cout<<i<<endl;
    return 0;
 }
